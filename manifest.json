{
	"manifest_version": 2,
	"name":"TabOne",
	"description": "OneTab aims to minimize browser tab ADD and allow you to actually accomplish your internet tasks.",
	"version": "0.1",
	"permissions": [
		"tabs",
		"storage"
	],
	"background": {
		"persistent": false,
		"scripts": ["OneTab.js"]
	}
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "OneTab.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
